<UserControl x:Class="OMNIToolNetworkAnalyzer.Presentation.Main.About.OMNIAReleaseNotes.Presentation.Main.MainReleaseNotesView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:cal="http://www.caliburnproject.org"      
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="350">
    <Grid Background="White">
        <TabControl>
            <TabItem Header="OMNIA release notes">
                <ContentControl cal:View.Model="{Binding ReleaseNotesViewModel}"/>
            </TabItem>
            <TabItem Header="Micro parser (*admin only)">
                <ContentControl cal:View.Model="{Binding MicroViewModel}"/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>